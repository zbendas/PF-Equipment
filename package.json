    "@vue/eslint-config-prettier": "^3.0.0-beta.10",
    "jsdoc": "^3.5.5",
    "jsdoc-vue": "^1.0.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.16"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
